{
    "name": "Trello Card Clean Up",
    "version": "1",
    "manifest_version": 2,
    "description": "Clean up dem cards...",
    "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": true
    },
    "browser_action": {
      "default_title": "Inject!"
    },
    "permissions": [
      "https://trello.com/c/*",
      "tabs"
    ]
  }